{
    "editor.formatOnSave": true,
    "python.analysis.extraPaths": [
        "./build/library/Debug"
    ],
    "python.analysis.exclude": [
        "vendor/sdl",
        "vendor/cpython",
        "build"
    ],
    "flake8.args": [
        "--max-line-length=120",
        "--ignore=E203,E301,E302,E305,W503,E701,E704"
    ],
    "ruff.lineLength": 120,
    "cmake.debugConfig": {
        "args": [
            "test"
        ]
    },
    "C_Cpp.codeAnalysis.exclude": {
        "vendor/*": true
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.clang_format_style": "file",
    "files.associations": {
        "*.embeddedhtml": "html",
        "*.sha": "hlsl",
        "*.shader-template": "hlsl",
        "iosfwd": "cpp",
